{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "warn", // console.log
        "no-unused-vars": "warn" // Неиспользуемые объявления
        // "react/jsx-first-prop-new-line": [1, "multiline"],
        // "react/jsx-max-props-per-line": [1,
        //     {
        //         "maximum": 1
        //     }
        // ]
    }
}
